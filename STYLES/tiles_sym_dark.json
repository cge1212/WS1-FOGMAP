{
  "version": 8,
  "metadata": {},
  "sources": {
    "ne2_shaded": {
      "maxzoom": 6,
      "tileSize": 256,
      "tiles": [
        "https://tiles.openfreemap.org/natural_earth/ne2sr/{z}/{x}/{y}.png"
      ],
      "type": "raster"
    },
    "openmaptiles": {
      "type": "vector",
      "url": "https://tiles.openfreemap.org/planet"
    }
  },
  "sprite": "https://tiles.openfreemap.org/sprites/ofm_f384/ofm",
  "glyphs": "https://tiles.openfreemap.org/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-color": "#232121"}
    },
    {
      "id": "park",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "park",
      "paint": {
        "fill-color": "#666666",
        "fill-opacity": 1,
        "fill-outline-color": "#666666"
      }
    },
    {
      "id": "landcover_wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "wood"]],
      "paint": {
        "fill-antialias": false,
        "fill-color": "#666666",
        "fill-opacity": 1
      }
    },
    {
      "id": "landcover_grass",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["==", "class", "grass"]],
      "paint": {
        "fill-antialias": false,
        "fill-color": "#666666",
        "fill-opacity": 1
      }
    },
    {
      "id": "landcover_wetland",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "minzoom": 12,
      "filter": ["all", ["==", "class", "wetland"]],
      "paint": {
        "fill-antialias": true,
        "fill-opacity": 1,
        "fill-translate-anchor": "map",
        "fill-color": "#666666"
